.progress {
  margin-top: 0rpx;
}
.back {
  font-size: 60rpx;
  margin: 20rpx 0 0 30rpx;
}
.login-title {
  font-size: 70rpx;
  margin: 70rpx 0 0 100rpx;
}
.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.login {
  position: absolute;
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.login-main {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 20rpx 70rpx;
}
.login-main .login-list {
  margin-top: 35rpx;
  height: 90rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 30rpx;
}
.login-main .login-list.border-all:after {
  border: 1px solid #D0D0D0;
  border-radius: 100rpx;
}
.login-main .login-list .iconfont {
  width: 65rpx;
  font-size: 44rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.login-main .login-list .iconfont:after {
  margin-left: 20rpx;
  content: '';
  width: 2rpx;
  height: 35rpx;
  background: #D0D0D0;
  display: block;
}
.login-main .login-list .login-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.login-main .login-list .login-input input {
  font-size: 28rpx;
  color: #333333;
  padding-left: 20rpx;
}
.login-main .login-list .code-sx {
  content: '';
  width: 2rpx;
  height: 25rpx;
  background: #D0D0D0;
  margin-right: 25rpx;
}
.login-main .login-list .codeimg {
  font-size: 24rpx;
  color: #999999;
}
.login-main .login-btn {
  margin-top: 70rpx;
  height: 96rpx;
  width: 100%;
  background: #1CBBB4;
  border-radius: 47rpx;
  font-size: 34rpx;
  color: #ffffff;
}

